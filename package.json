{
  "name": "@mudano/ui-react",
  "version": "5.2.0",
  "description": "Mudano UI React Component Library",
  "main": "dist/ui-react.min.js",
  "modules.root": "lib",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mudano/ui-react.git"
  },
  "author": "Mudano Ltd. (http://mudano.com)",
  "license": "Apache-2.0",
  "homepage": "https://mudano.github.io/ui-react/",
  "bugs": {
    "url": "https://mdshowto.atlassian.net/projects/UR/issues",
    "email": "ui-react@mudano.com"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "start": "yarn storybook:watch",
    "dev": "yarn start",
    "watch": "yarn start",
    "clean": "rimraf dist",
    "build": "npm-run-all -s lint clean build:umd build:commonjs",
    "build:umd": "webpack",
    "build:commonjs": "babel src --out-dir dist --ignore storybook-addons/*,*.story.js,*.test* --plugins=remove-sass && node-sass src -o dist",
    "release:prepare": "node bin/prepare-release",
    "release:publish": "node bin/publish-release",
    "storybook:clean": "rimraf ./storybook-static",
    "storybook:build": "yarn lint && build-storybook -o ./storybook-static",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:watch": "start-storybook -p 6006",
    "lint": "npm-run-all -s lint:scss lint:js",
    "lint:scss": "stylelint './src/**/*.scss'",
    "lint:scss-fix": "yarn lint:scss --fix",
    "lint:js": "eslint ./src --cache",
    "lint:js-fix": "yarn lint:js --fix",
    "test": "npm-run-all -s test:unit test:int",
    "test:unit": "cross-env NODE_ENV=test karma start karma.config.js --autoWatch false --singleRun true",
    "test:unit-watch": "cross-env NODE_ENV=test karma start karma.config.js --autoWatch true --singleRun false",
    "test:int": "codeceptjs run",
    "test:int-setup": "./test/codecept/setup"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.3.3",
    "@storybook/addon-options": "^3.2.17",
    "@storybook/addons": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-react-docgen": "^1.8.1",
    "babel-plugin-remove-sass": "^0.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "base64-inline-loader": "^1.1.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-0",
    "clone": "^2.1.1",
    "codeceptjs-nightmare": "^1.1.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "^1.1.6",
    "github": "^13.1.0",
    "graphql": "^0.11.0",
    "graphql-tag": "^2.6.1",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "moment": "^2.20.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.0.0",
    "npm-run-all": "^4.1.2",
    "npm-utils": "^2.0.0",
    "react": ">=0.14.0 <= 15",
    "react-dom": ">=0.14.0 <= 15",
    "react-showdown": "^1.6.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "<= 15",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.3.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0 <= 16",
    "react-dom": ">=0.14.0 <= 16"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "classnames": "^2.2.5",
    "flatpickr": "4.3.2",
    "prop-types": "15.6.0"
  }
}
